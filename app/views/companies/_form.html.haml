= form_for @company do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :phone
    %br
    = f.text_field :phone
  %p
    = f.label :address
    %br
    = f.text_field :address
  %p
    = f.label :city
    %br
    = f.text_field :city
  %p
    = f.label :country
    %br
    = f.text_field :country
  %p
    = f.submit
